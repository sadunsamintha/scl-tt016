  <monitoringFrequencyHighInSec>60</monitoringFrequencyHighInSec>
  <requestAlarmListFrequencyInSec>5</requestAlarmListFrequencyInSec>
  <requestStatusFrequencyInSec>1</requestStatusFrequencyInSec>
  <mPrinterParameters class="com.sicpa.standard.printer.driver.DominoParameter">
  		<mCodeType>EXTENDED_CODE</mCodeType>
  		<mDataMatrixFormat>DM_8x18</mDataMatrixFormat>
  		<mDataMatrixEncoding>ASCII</mDataMatrixEncoding>
	    <mFontType>PIXEL_5x5</mFontType>
		<mTextBlockSpacing>1</mTextBlockSpacing>
