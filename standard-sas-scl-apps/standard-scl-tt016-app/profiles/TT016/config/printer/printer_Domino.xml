<DominoPrinterModel>
  <name>SCD2</name>
  <ipAdress>192.168.1.4</ipAdress>
  <deviceId>1234567890123456789012345678901234567890</deviceId>
  <udpPort>4321</udpPort>
  <tcpTimeout>10000</tcpTimeout>
  <tcpPort>1234</tcpPort>
  <requestReservoirHoursLeftInterval>30</requestReservoirHoursLeftInterval>
  <mUsePrinterSimulator>false</mUsePrinterSimulator>
  <resetOnStop>true</resetOnStop>
  <mDebugMode>false</mDebugMode>
  <monitoringSnmpPort>161</monitoringSnmpPort>
