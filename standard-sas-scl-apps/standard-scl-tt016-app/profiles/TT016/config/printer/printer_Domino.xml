  		<mTextFormat>TF_2_LINES</mTextFormat>
  		<isPrinterBufferSizeControlled>true</isPrinterBufferSizeControlled>
  		<noFlushOnStop>false</noFlushOnStop>
		<mCodeSizePerRequest>100</mCodeSizePerRequest>
		<mCodeBufferLowThreshold>20</mCodeBufferLowThreshold>
