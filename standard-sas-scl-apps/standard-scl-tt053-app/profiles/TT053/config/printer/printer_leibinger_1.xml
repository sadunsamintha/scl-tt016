<LeibingerPrinterModel>
	<name>LEIBINGER</name>
	<ipAdress>192.168.1.11</ipAdress>
	<deviceId>1</deviceId>
	<udpPort>4321</udpPort>
	<tcpTimeout>5000</tcpTimeout>
	<tcpPort>3000</tcpPort>
	<tcpMaxExpiredRequests>3</tcpMaxExpiredRequests>
	<transitionMaxExpiredRequests>10</transitionMaxExpiredRequests>
	<resetOnStop>true</resetOnStop>
	<requestBufferStatusPeriod>500</requestBufferStatusPeriod>
	<nozzleMode>OPEN_MANUAL_CLOSE_AUTO</nozzleMode>
	<nozzleCloseDelayMs>1000</nozzleCloseDelayMs>
	<fontType>PIXEL_7x5</fontType>
	<securePing>true</securePing>
	<requestMonitoringLowFrequency>86400000</requestMonitoringLowFrequency>
	<requestMonitoringMediumFrequency>3600000</requestMonitoringMediumFrequency>
	<requestMonitoringHighFrequency>300000</requestMonitoringHighFrequency>
	<requestStatusPeriod>2000</requestStatusPeriod>
	<allowedInkTags>79000-*</allowedInkTags>
	<allowedSolventTags>77001-*</allowedSolventTags>
	
	<printerParameters class="com.sicpa.standard.printer.leibinger.model.LeibingerParameter">
		<codeType>DYNAMIC_DATAMATRIX</codeType>
		<dataMatrixFormat>DM8x18</dataMatrixFormat>
		<dataMatrixEncoding>ASCII</dataMatrixEncoding>
	</printerParameters>
	<wysiwyg>true</wysiwyg>
	<!-- 
		Encoder resolution (pulses/meter)
	 -->
	<encoderResolution>9000</encoderResolution>
	<!--  
		Mirror-inverted (Y-axis)
		0 = no, 1 = yes
	-->
	<inverted>0</inverted>
	<!-- 
		Rotation in degrees anticlockwise
		# 0, 90, 180 or 270 (180 = reverse + inverted)
	 -->
	<rotation>0</rotation>

	<!--
		HRC - Mirror-inverted (Y-axis)
		0 = no, 1 = yes
	-->
	<invertedDataMatrix>false</invertedDataMatrix>
	<reversedDataMatrix>false</reversedDataMatrix>
	<invertedText>false</invertedText>
	<reversedText>true</reversedText>
	<invertedBitmapLogo>false</invertedBitmapLogo>
	<reversedBitmapLogo>false</reversedBitmapLogo>
	<!--
		Encoder Signal
		0 = Internal
		1 = External, direction 1 (clockwise)
		2 = External, direction 2 (anticlockwise)
		3 = External, both directions
	-->
	<encoderSignal>1</encoderSignal>
	<!-- 
		PrintGo Signal active
		0 = internal PrintGo
		1 = external pos-edge
		2 = external neg-edge 
	-->
	<printGoSignal>1</printGoSignal>
	<!--  Print Height in % (0-100) -->
	<printHeight>100</printHeight>
	<!--  Flight Time Compensation in micrometers -->
	<flightTimeCompensationMicrometers>4000</flightTimeCompensationMicrometers>
	<!-- 
		Print Mode Shall be compatible with the height of the datamatrix
		Leibinger-Jet3 Script doc V1.6.0.10:
		
		1 = 32 Dot Fast
		2 = 32 Dot High Speed
		3 = 24 Dot High Quality
		4 = 24 Dot Fast
		5 = 24 Dot High Speed
		6 = 24 Dot High Speed Barcode
		7 = 16 Dot Fast
		8 = 16 Dot Speed
		9 = 12 Dot High Speed
		10 = 5 Dot High Speed
		11 = 2 Lines High Speed
	 -->
	<printMode>9</printMode>
	<!-- Stroke Distance in micrometers -->
	<strokeDistanceMicrometers>500</strokeDistanceMicrometers>
	<!-- Print Go delay in micrometers -->
	<printGoDelayMicrometers>6000</printGoDelayMicrometers>
	<printGoDistance>0</printGoDistance>
	<printGoRepetition>0</printGoRepetition>
    <backwardLock>1</backwardLock>
    <printGoHoldOff>60000</printGoHoldOff>
</LeibingerPrinterModel>