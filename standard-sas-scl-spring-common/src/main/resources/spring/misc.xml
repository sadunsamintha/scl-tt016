<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="exceptionHandler" class="${exceptionHandler}" />

	<bean id="eventBusAutoRegistering" class="${eventBusAutoRegistering}" />

	<bean id="productionParameters" class="${productionParameters}" />

	<bean id="applicationInitializationTasks" class="${applicationInitializationTasks}" />
	
	<bean id="implementationProvider" class="${implementationProvider}" />

</beans>
