<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


	<bean id="skuListProvider" class="${skuListProvider}" />

	<bean id="authenticatorProvider" class="${authenticatorProvider}" />

	<bean id="authenticatorModeProvider" class="${authenticatorModeProvider}" />

	<bean id="productionBatchProvider" class="${productionBatchProvider}" />

	<bean id="plcProvider" class="${plcProvider}" />

	<bean id="plcSecureProvider" class="com.sicpa.standard.sasscl.provider.impl.PlcProvider" />

	<bean id="productionConfigProvider" class="${productionConfigProvider}" />

	<bean id="activationBehaviorProvider" class="${activationBehaviorProvider}" />

	<bean id="subsystemIdProvider" class="${subsystemIdProvider}">
		<constructor-arg value="${subsystemId}" />
	</bean>

</beans>


