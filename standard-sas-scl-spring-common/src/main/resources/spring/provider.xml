<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


	<bean id="skuListProvider" class="com.sicpa.standard.sasscl.provider.impl.SkuListProvider" />

	<bean id="authenticatorProvider" class="com.sicpa.standard.sasscl.provider.impl.AuthenticatorProvider" />

	<bean id="authenticatorModeProvider" class="com.sicpa.standard.sasscl.provider.impl.AuthenticatorModeProvider" />

	<bean id="productionBatchProvider" class="com.sicpa.standard.sasscl.provider.impl.ProductionBatchProvider" />

	<bean id="plcProvider" class="com.sicpa.standard.sasscl.provider.impl.PlcProvider" />

	<bean id="plcSecureProvider" class="com.sicpa.standard.sasscl.provider.impl.PlcProvider" />

	<bean id="productionConfigProvider" class="com.sicpa.standard.sasscl.provider.impl.ProductionConfigProvider" />

	<bean id="activationBehaviorProvider" class="com.sicpa.standard.sasscl.provider.impl.ActivationBehaviorProvider" />

	<bean id="subsystemIdProvider" class="com.sicpa.standard.sasscl.provider.impl.SubsystemIdProvider">
		<constructor-arg value="${subsystemId}" />
	</bean>

</beans>


