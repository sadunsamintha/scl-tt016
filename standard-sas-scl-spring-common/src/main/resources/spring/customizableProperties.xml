<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


	<bean id="customizablePropertyDefinition" class="${customizablePropertyDefinition}">
	</bean>

	<bean
		class="org.springframework.beans.factory.config.MethodInvokingFactoryBean">
		<property name="staticMethod"
			value="com.sicpa.standard.sasscl.model.custom.CustomizablePropertyFactory.setCustomizablePropertyDefinition" />
		<property name="arguments">
			<list>
				<ref bean="customizablePropertyDefinition" />
			</list>
		</property>
	</bean>

</beans>
