<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.0.xsd">

	<!-- ******************************************************* -->
	<!-- CABINET PARAMETERS -->
	<!-- ******************************************************* -->

	<!-- variables list -->
	<bean id="cabinetEditablePlcVariables" class="com.sicpa.standard.sasscl.devices.plc.variable.EditablePlcVariables">
		<constructor-arg ref="allPlcVariableValues" />

		<constructor-arg>
			<array>
				<bean id="plcGrp9 " class="com.sicpa.standard.sasscl.devices.plc.variable.PlcVariableGroup">
					<constructor-arg>
						<array>
							<ref bean="PARAM_CAB_TIMEOUT_LIFECHECK_desc" />
							<ref bean="PARAM_CAB_COOLING_ERR_ACTIVATION_TIMEOUT_desc" />
							<ref bean="PARAM_CAB_COM_STRUC_STORED_IN_RAM_desc" />
							<ref bean="PARAM_CAB_INHIBIT_RAM_WRITING_desc" />
							<ref bean="PARAM_CAB_MULTILINE_REQ_MODE_ENABLED_desc" />
						</array>
					</constructor-arg>
					<property name="description" value="plc.config.cabinet.system" />
				</bean>
				<bean id="plcGrp10" class="com.sicpa.standard.sasscl.devices.plc.variable.PlcVariableGroup">
					<constructor-arg>
						<array>
							<ref bean="PARAM_CAB_WAR_TEMP_EE_CAB_LVL_desc" />
							<ref bean="PARAM_CAB_WAR_TEMP_AMBIANT_LVL_desc" />
							<ref bean="PARAM_CAB_WAR_TEMP_BYPASS_LVL_desc" />
							<ref bean="PARAM_CAB_ERR_TEMP_EE_CAB_LVL_desc" />
							<ref bean="PARAM_CAB_ERR_TEMP_AMBIANT_LVL_desc" />
							<ref bean="PARAM_CAB_ERR_TEMP_BYPASS_LVL_desc" />
						</array>
					</constructor-arg>
					<property name="description" value="plc.config.cabinet.temp" />
				</bean>
				<bean id="plcGrp11" class="com.sicpa.standard.sasscl.devices.plc.variable.PlcVariableGroup">
					<constructor-arg>
						<array>
							<ref bean="PARAM_CAB_WAR_AIR_PRESS_LVL_desc" />
							<ref bean="PARAM_CAB_ERR_AIR_PRESS_LVL_desc" />
						</array>
					</constructor-arg>
					<property name="description" value="plc.config.cabinet.air" />
				</bean>

				<bean id="plcGrp12" class="com.sicpa.standard.sasscl.devices.plc.variable.PlcVariableGroup">
					<constructor-arg>
						<array>
							<ref bean="PARAM_CAB_WAR_FANS_EE_CAB_MIN_FREQ_desc" />
							<ref bean="PARAM_CAB_WAR_FANS_EE_CAB_MAX_FREQ_desc" />
							<ref bean="PARAM_CAB_WAR_FANS_BYPASS_CAB_MIN_FREQ_desc" />
							<ref bean="PARAM_CAB_WAR_FANS_BYPASS_CAB_MAX_FREQ_desc" />
							<ref bean="PARAM_CAB_FANS_TEMP_ON_THRESHOLD_EE_desc" />
							<ref bean="PARAM_CAB_FANS_TEMP_OFF_DELTA_HYST_EE_desc" />
							<ref bean="PARAM_CAB_FANS_TEMP_ON_THRESHOLD_BYPASS_desc" />
							<ref bean="PARAM_CAB_FANS_TEMP_OFF_DELTA_HYST_BYPASS_desc" />
						</array>
					</constructor-arg>
					<property name="description" value="plc.config.cabinet.fan" />
				</bean>

			</array>
		</constructor-arg>

		<property name="file" value="${profile.path}/config/plc/cabinetConfig.xml" />

	</bean>

	<!--variable values -->
	<bean id="allPlcVariableValues" class="com.sicpa.standard.client.common.utils.ConfigUtils" factory-method="load">
		<constructor-arg value="${profile.path}/config/plc/cabinetConfig.xml" />
	</bean>

</beans>
