<com.sicpa.standard.printer.oi2jet.driver.Oi2jetParameter>
	<codeType>DSL</codeType>
	<dataMatrixFormat>DM8x18</dataMatrixFormat>
	<dataMatrixEncoding>ASCII</dataMatrixEncoding>
	<textFormat>TF_1_LINE</textFormat>
	<textFontName>"Courier New"</textFontName>
	<textFontSize >50</textFontSize>
	<horizontalResolution>300</horizontalResolution>
	<verticalResolution>600</verticalResolution>
	<interlaced>true</interlaced>
	<datamatrixPixelsPerDot>10</datamatrixPixelsPerDot>
	<startPrintWaitDistance>13000</startPrintWaitDistance>
	<rotate90Degrees>false</rotate90Degrees>
	<codeBufferLowThreshold>100</codeBufferLowThreshold>
	<codeSizePerRequest>1000</codeSizePerRequest>
	<printerBufferLowThreshold>200</printerBufferLowThreshold>
	<nbCodesPerSending>100</nbCodesPerSending>
	<requestBufferStatusPeriod>500</requestBufferStatusPeriod>
	<dslScript>canvas.alocateMM(20)
	.usingFontResource("/OCRA.ttf", 30)
	.at(1.8, 1.8)
	//.DMTX( param_1, 15, \"DM16x16\" )
	.at(1, 7).rotate(-45).QrCode(param_1, 10).rotate(0)
	//.QrCode(param_1, 12)
	.at(0.0,0.0).rectangle(19.9, 17.5 )
	.at(1,1).rectangle(1,1)
	.at( 1.8,  17.5+1.5 )
	.write(param_2)
	.at( 15+2, 17.5-(1.8*2) )
	.fontSize(50)
	.rotate(-90)
	.write(param_3);
	</dslScript>
	
</com.sicpa.standard.printer.oi2jet.driver.Oi2jetParameter>
